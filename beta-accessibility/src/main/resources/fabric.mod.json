{
  "schemaVersion": 1,
  "id": "beta-accessibility",
  "version": "${version}",
  "environment": "client",
  "name": "Fabric Beta Essentials Accessibility",
  "description": "A library of client accessibility features, such as localization and sound categories.",
  "authors": [
    "Cursed Legacy",
    "Better Than Updates"
  ],
  "contact": {
    "homepage": "https//better-than-updates.halotroop.com/",
    "issues": "https://github.com/Better-Than-Updates-MC/API/issues",
    "sources": "https://github.com/Better-Than-Updates-MC/API"
  },

  "license": "MIT",
  "icon": "assets/beta-accessibility/icon.png",
  "entrypoints": {
    "client": [
      "com.halotroop.fabric.impl.translations.TranslationsInitializer"
    ]
  },
  "mixins": [
    "beta-translations.accessors.json",
    "beta-translations.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=1.0.0",
    "beta-api-base": "*",
    "minecraft": "1.0.0-beta.7.3"
  },
  "breaks": {
    "stationloader": "*"
  }
}

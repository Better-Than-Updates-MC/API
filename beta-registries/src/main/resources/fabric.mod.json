{
  "schemaVersion": 1,
  "id": "beta-registries",
  "version": "${version}",

  "name": "Fabric Beta Registries API",
  "description": "An API required to safely register new game objects without conflicts.",
  "authors": [
    "Cursed Legacy",
    "Better Than Updates"
  ],
  "contact": {
    "homepage": "https://better-than-updates.halotroop.com/",
    "issues": "https://github.com/Better-Than-Updates-MC/API/issues",
    "sources": "https://github.com/Better-Than-Updates-MC/API"
  },

  "license": "MIT",
  "icon": "assets/api/icon.png",
  
  "entrypoints": {
    "init": [
      "com.halotroop.fabric.impl.registry.RegistryImpl"
    ]
  },
  
  "mixins": [
    "beta-registries.accessors.json",
    "beta-registries.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=1.0.0",
    "beta-api-base": "*",
    "minecraft": "1.0.0-beta.7.3"
  },
  "breaks": {
    "stationloader": "*"
  }
}
